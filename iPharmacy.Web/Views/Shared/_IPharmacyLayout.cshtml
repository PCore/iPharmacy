@{
    ViewBag.Title = "_IPharmacyLayout";
}
<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8" />
        <title>iPharmacy </title>
        <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
        <meta name="viewport" content="width=device-width" />
        @Styles.Render("~/Content/css")
        @Scripts.Render("~/bundles/modernizr")
    </head>
    <body>
        <header>
        
            <div class="content-wrapper">               
              <font class="titlestyle"> Web Samples Store </font>  
              <hgroup class="title">
                  <ul id="menunew">
                            <li>@Html.ActionLink("Home", "Index", "Home")</li>
                            <li>@Html.ActionLink("Product", "Index", "Product")</li>
                            <li>@Html.ActionLink("About", "About", "Home")</li>
                            <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
             </ul>
                      
            </hgroup>    
            <div style="float:right">
                    <section id="login">
                        @Html.Partial("_LoginPartial")
                    </section>
            </div>
                               
            </div>
           
            
               
               
        </header>
       
        <div id="body">
            @RenderSection("featured", required: false)
           
            <section class="content-wrapper main-content clear-fix">
              
                @RenderBody()
            </section>
          
        </div>
    <footer>
            
            <div class="content-wrapper">        
             <div style="margin:10px"> <p style="width:350px;float:right">&copy; The information provided in this site is intended only for professionals.

The products discussed herein may have different product labeling in different countries.</p></div>     
                   
            
            </div>
        </footer>

        @Scripts.Render("~/bundles/jquery")
        @RenderSection("scripts", required: false)
    </body>
</html>
